{
    "name": "ashiful/admin",
    "description": "Dynamic Admin Panel",
    "keywords": [
        "ashiful",
        "admin"
    ],
    "homepage": "https://github.com/ashiful/admin",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ashiful Islam",
            "email": "ashiful134@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "symfony/dom-crawler": "~3.1|~4.0|~5.0|~6.0",
        "illuminate/support": "^8.0",
        "doctrine/dbal": "2.*|3.*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ashiful\\Admin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ashiful\\Admin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ashiful\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Ashiful\\Admin\\AdminFacade"
            }
        }
    }
}
